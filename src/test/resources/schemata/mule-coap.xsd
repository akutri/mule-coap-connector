        <xs:extension base="mule:abstractExtensionType">
          <xs:annotation>
            <xs:documentation>The Client is used to send requests to resources on CoAP servers
              and/or to observe these. The CoAP server address (host, port) configured here is
              default for the requests using this client. The request can override if
              necessary.</xs:documentation>
          </xs:annotation>
          <xs:sequence minOccurs="0" maxOccurs="1">
            <xs:element minOccurs="0" maxOccurs="1" name="endpoint">
              <xs:annotation>
                <xs:documentation>Endpoint configuration to use for the client.</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:choice minOccurs="1" maxOccurs="1">
                  <xs:element xmlns:coap="http://www.teslanet.nl/schema/mule/coap" minOccurs="1"
                    maxOccurs="1" ref="coap:abstract-endpoint"/>
                  <xs:element xmlns:coap="http://www.teslanet.nl/schema/mule/coap" minOccurs="1"
                    maxOccurs="1" ref="coap:global-abstract-endpoint"/>
                </xs:choice>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
          <xs:attribute type="xs:string" use="required" name="name"/>
          <xs:attribute type="xs:string" use="optional" name="endpoint">
            <xs:annotation>
              <xs:documentation>Endpoint configuration to use for the client.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute type="xs:string" use="optional" name="host">
            <xs:annotation>
              <xs:documentation>The default hostname or ip of the server to address. Can be
                overridden by the host setting on operations.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute type="mule:substitutableInt" use="optional" name="port">
            <xs:annotation>
              <xs:documentation>The default port the server is listening on. Can be overridden by
                the port setting on the operation.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:element xmlns="http://www.teslanet.nl/schema/mule/coap"
